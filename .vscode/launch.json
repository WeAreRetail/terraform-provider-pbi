{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Terraform Provider",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // this assumes your workspace is the root of the repo
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}/examples/resources/powerbi_workspace",
            "env": {
                "GODEBUG": "netdns=cgo"
            },
            "args": [
                "-debug",
            ]
        }
    ]
}
